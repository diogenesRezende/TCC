package br.edu.univas.restapiapp.model;

import java.math.BigDecimal;
import java.util.Date;

import javax.persistence.EntityManager;
import javax.persistence.EntityTransaction;
import javax.persistence.Persistence;

import br.edu.univas.restapiapp.util.JpaUtil;

public class CreateTable {

	public static void main(String[] args) {
		//

		EntityManager manager = JpaUtil.getEntityManager();
		EntityTransaction tx = manager.getTransaction();
		tx.begin();
		
		Aluno aluno = new Aluno();
		aluno.setEmail("diogenes@gmail.com");
		aluno.setIdDbExterno(98004095L);
		aluno.setNome("Di√≥genes Aparecido Rezende");
		aluno.setPeriodo(8);
		manager.persist(aluno);
		
		Aluno aluno1 = new Aluno();
		aluno1.setEmail("diego@gmail.com");
		aluno1.setIdDbExterno(98004096L);
		aluno1.setNome("Diego Nunes");
		aluno1.setPeriodo(8);
		
		manager.persist(aluno1);
		
		Disciplina disciplina = new Disciplina();
		disciplina.setDescricao("Engenharia de Software I");
		disciplina.setIdDbExterno(1032l);
		disciplina.setNome("Engenharia de Software");
		manager.persist(disciplina);
	
		Disciplina disciplina1 = new Disciplina();
		disciplina.setDescricao("Engenharia de Software II");
		disciplina.setIdDbExterno(1033l);
		disciplina.setNome("Engenharia de Software II");
		manager.persist(disciplina1);
		
		Evento evento =new Evento();
		evento.setAluno(aluno);
		evento.setDataEfetiva(new Date());
		evento.setDataLancamento(new Date());
		evento.setDisciplina(disciplina);
		evento.setNota(BigDecimal.valueOf(30.0));
		evento.setTipo("Prova");
		evento.setTipoEvento(TipoEvento.PROVA_AGENDADA);
		
	
		tx.commit();
		manager.clear();
		JpaUtil.close();

	}

}
